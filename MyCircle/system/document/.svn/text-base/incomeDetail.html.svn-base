<script type="text/javascript" src="../scripts/app.js"></script>
<script type="text/javascript" src="../scripts/json2.js"></script>
<script type="text/javascript">
var isAPP = (window.APP && window.APP.login) ? true : false, // 是否通过手机访问
	userName = isAPP ? window.APP.getUserName() : "",    // 获得登录账户的用户名
	pageUrl = window.location.href.toString(),
	data = {
		"do":app.getParam(pageUrl, "do"),
		"status": app.getParam(pageUrl, "status"),
		"id": app.getParam(pageUrl, "id")
	},
	setResult = function (rs, handler) {
		if (isAPP) {
			window.APP.hideLoading();
		}
		if (!rs.success && rs.errorCode == "001") {
			if (isAPP) {
				window.APP.login();
			} else {
				alert(rs.message);
			}
		} else {
			if (typeof handler == "function") {
				handler(rs);
			}
		}
	};

if (isAPP && !window.APP.isSupportPramas()) {
	app.setData(window.APP.getParams());
}
	
function load () {
	app.loadData(app.basePath + "TdocIncoming/tdocincoming!viewAjax.action", data, function (rs) {
		setResult(rs, setIncomeDetail);
	});
}

$(function () {
	load();
	if (data.do == "true") {
		$(".ft.eoa-form").show();
	} else {
		$(".info-mini").hide();
		$(".info-more").show();
	}
	$(".info-mini").click(function () {
		$(this).hide();
		$(".info-more").show();
	});
	$(".bd iframe").css({
		"marginLeft":10,
		"width":$("#pageindex").width() - 20,
		"height":$(window).height() - $(".hd").height() - 80
	});
});
</script>
</head>
<body id="document">
<div id="pageindex" class="theme-default">
