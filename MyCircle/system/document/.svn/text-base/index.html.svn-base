<!doctype html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,minimum-scale=1.0,user-scalable=no" />
<meta name="format-detection" content="telephone=no" />
<meta name="format-detection" content="email=no" />
<meta name="format-detection" content="address=no;">
<meta name="apple-mobile-web-app-capable" content="yes" />
<meta name="apple-mobile-web-app-status-bar-style" content="default" />
<title>公文管理</title>
<link rel="stylesheet" type="text/css" href="../css/global.css"/>
<script type="text/javascript" src="../scripts/jquery-1.8.2.min.js"></script>
<script type="text/javascript" src="../scripts/app.js"></script>
<script type="text/javascript">
var isAPP = (window.APP && window.APP.login) ? true : false, // 是否通过手机访问
	userName = isAPP ? window.APP.getUserName() : "",    // 获得登录账户的用户名
	pageUrl = window.location.href.toString(),
	data = {
		"diaType": app.getParam(pageUrl, "diaType") || "",
		"page.pageNo": app.getParam(pageUrl, "page.pageNo") || 1,
		"page.pageSize": app.getParam(pageUrl, "page.pageNo") || 10,
		"page.order": "desc",
		"page.orderBy": "diaDate"
	},
	setResult = function (rs, handler) {
		if (isAPP) {
			window.APP.hideLoading();
		}
		if (!rs.success && rs.errorCode == "001") {
			if (isAPP) {
				window.APP.login();
			} else {
				alert(rs.message);
			}
		} else {
			if (typeof handler == "function") {
				handler(rs);
			}
		}
	};
	

$(function () {


});
</script>
</head>
<body id="document" style="background:#F5F5F5">
<div id="pageindex" class="theme-default" style="background:#F5F5F5">
	<div class="function">
		<ul>
			<li class="document-send-todo" ><a href="todo.html?type=send" class="link">待办发文<span class="arr">></span></a></li>
			<li class="document-send-done" ><a href="done.html?type=send" class="link">已办发文<span class="arr">></span></a></li>
		</ul>
		<ul>
			<li class="document-income-todo" ><a href="todo.html?type=income" class="link">待办收文<span class="arr">></span></a></li>
			<li class="document-income-done" ><a href="done.html?type=income" class="link">已办收文<span class="arr">></span></a></li>
		</ul>
	</div>
	
</div>
</body>
</html>