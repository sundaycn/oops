*{ margin:0; padding:0;font-size:14px;-webkit-tap-highlight-color: rgba(0,0,0,0.1);}
*:focus {outline: none;}
a{text-decoration:none;color:#000;}
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video,p{margin:0;padding:0;border:0;outline:0;font-size:100%;font-weight:normal;vertical-align:baseline;background:transparent;-webkit-text-size-adjust:none;}
body{line-height:1;}
nav ul{list-style:none;}
li{list-style:none;}
h1,h2,h3,h4,h5,h6{font-style:normal;}
blockquote,q{quotes:none;}
blockquote:before,blockquote:after,q:before,q:after{content:'';content:none;}
a{margin:0;padding:0;font-size:100%;vertical-align:baseline;background:transparent;}
ins{background-color:#ff9;color:#000;text-decoration:none;}
mark{background-color:#ff9;color:#000;font-style:italic;font-weight:bold;}
del{text-decoration:line-through;}
abbr[title],dfn[title]{border-bottom:1px dotted;cursor:help;}
table{border-collapse:collapse;border-spacing:0;}
hr{display:block;height:1px;border:0;border-top:1px solid#cccccc;margin:1em 0;padding:0;}
textarea {font-size:14px;color:#666;}
@font-face {
	font-family: digits;
	src: url('../../fonts/digits3.ttf');
}
@font-face {
	font-family: thinfont;
	src: url('../../fonts/thinfont.ttf');
}
@font-face {
	font-family: simsun;
	src: url('../../fonts/simsun.ttc');
}
html, body {
	width:100%;
	min-height:100%;
	font-family:Helvetica, STHeiti STXihei, Microsoft JhengHei, Microsoft YaHei, Tohoma, Arial;
}
.clearfix:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
footer {
	display:none;
	position:absolute;
	left:10px;
	right:10px;
	bottom:5px;
	font-size:10px;
	color:#999;
	text-align:center;
}
.fl{
	float:left;
}
.clear{
	clear:both;
	display:block;
	height:0;
	line-height:0;
	font-size:0;
}
.pdlr {
	padding:0 10px;
}
.page {
	display:none;
	margin:10px 0;
}
.page .next {
	display:block;
	line-height:35px;
	text-align:center;
	background:#f4f4f4;
}
.page .next:hover {
	background:#e4e4e4;
}
.eoa-form {
	padding:10px;
}
.eoa-form ul {
	padding-bottom:10px;
}
.eoa-form li {
	padding:0 3px;
	display:-webkit-box;
	box-shadow:0 -1px 0px rgba(213,213,213,0.5) inset;
	min-height:45px;
	line-height:45px;
	margin:1px 0;
}
.eoa-form li .label,
.eoa-form li .input {
	display:block;
	-webkit-box-flex:1;
	-moz-box-flex:1;
}
.eoa-form li .input {
	position:relative;
}
.eoa-form li .input .icon {
	position:absolute;
	z-index:2;
}
.eoa-form li .input .icon-triangle {
	position:absolute;
	bottom:10px;
	right:0;
	width:0;
	height:0;
	border-top:6px solid #fff;
	border-left:6px solid #fff;
	border-right:6px solid #d4d4d4;
	border-bottom:6px solid #d4d4d4;
}
.eoa-form li .input .icon-star {
	color:#cc0000;
	top:15px;
	right:0px;
	line-height:0;
	font-size:20px;
}
.eoa-form li.error {
	background:#fff3f3;
}
.eoa-form li.error .input .icon-triangle {
	border-top:6px solid #fff3f3;
	border-left:6px solid #fff3f3;
}
.eoa-form li .textarea,
.eoa-form li .number {
	line-height:0;
}
.eoa-form li .label {
	font-size:16px;
	color:#8b8a8a;
	width:90px;
	min-width:90px;
	max-width:90px;
	text-align:right;
}
.eoa-form .checkbox {
	position:relative;
	display:inline-block;
	vertical-align:middle;
	font-size:16px;
	height:45px;
	margin-left:5px;
	margin-right:5px;
	cursor:pointer;
}
.eoa-form .checkbox .check {
	position:absolute;
	left:0;
	top:13px;
	display:inline-block;
	width:16px;
	height:16px;
	border:1px solid #0f82fe;
	border-radius:3px;
}
.eoa-form .checkbox.checked .check {
	background: #0f82fe url(../images/email_icon_selected.png) no-repeat center;
	background-size: 15px 15px;
}
.eoa-form .checkbox .str {
	padding-left:20px;
}

.eoa-form input,
.eoa-form textarea,
.eoa-form select,
.eoa-form .datepicker,
.eoa-form .timepicker{
	width:100%;
	height:45px;
	line-height:45px;
	border:none;
	background:none;
	font-size:16px;
	color:#222;
	cursor:pointer;
	-webkit-tap-highlight-color: rgba(0,0,0,0);
}
.eoa-form .datepicker,
.eoa-form .timepicker {
	display:block;
	
}
.eoa-form textarea {
	line-height:180%;
	padding-top:8px;
}
.eoa-form .selectbox {
	position:relative;
	line-height:100%;
	padding-top:8px;
	padding-right:45px;
}
.eoa-form .selectbox .ico {
	position:absolute;
	width:30px;
	height:45px;
	line-height:45px;
	top:0;
	right:0;
	text-align:center;
}
.eoa-form .selectbox .add {
	font-family:thinfont;
	font-size:30px;
	color:#52B78F;
	cursor:pointer;
}
.eoa-form .selectbox  .ser {
	position:relative;
	background:#68C069;
	color:#FFFFFF;
	font-style:normal;
	font-size:14px;
	padding:4px 8px;
	height:20px;
	line-height:20px;
	display:inline-block;
	margin-bottom:5px;
	margin-right:5px;
}
.eoa-form .selectbox  .ser .ico {
	display:none;
}
.eoa-form .selectbox  .del {

}
.eoa-form .selectbox  .del .ico {
	position:absolute;
	display:block;
	color:#fff;
	background:#cc0000;
	border-radius:10px;
	top:-5px;
	right:-2px;
	width:15px;
	height:15px;
	line-height:15px;
	font-size:10px;
	font-family:thinfont;
	font-style:normal;
	
}
.eoa-form .btn {
	background-color:rgb(201,169,108);
	display:block;
	height:45px;
	line-height:45px;
	text-align:center;
	text-decoration:none;
	font-size:20px;
	color:#fff;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}

.eoa-data-list {
	
}
.eoa-data-list > ul > li {
	position:relative;
	box-shadow:0 -1px 0px rgba(213,213,213,0.5) inset;
}
.eoa-data-list > ul > li.add{
	background:rgba(227,245,234, 0.3);
}
.eoa-data-list li .t {
	padding:5px 0;
	padding-left:30px;
	min-height:55px;
	line-height:55px;
	cursor:pointer;
	color:#2f2f2f;
	display:block;
}
.eoa-data-list li .t h3 {
	line-height:120%;
	padding:8px 0;
	font-size:16px;
	color:#2F2F2F;
	vertical-align:middle;
}
.eoa-data-list li .t.add {
	color:#89CDA7;
	min-height:50px;
	line-height:50px;
}
.eoa-data-list li .t p {
	line-height:14px;
	font-size:12px;
	color:#8D8D8D;
	padding-right:15px;
}
.eoa-data-list li .t p .date {
	color:#89CDA7;
	font-style:normal;
}
.eoa-data-list > ul > li > span {
	position:absolute;
}
.eoa-data-list li .bubble {
	top:20px;
	right:10px;
	text-align:center;
	font-family:thinfont;
	font-size:12px;
	color:#fff;
	padding:5px 10px;
	border-radius:60px;
	background:#28b198;
}
.eoa-data-list li .bubble.red {
	background:#f97e76;
}
.eoa-data-list li .arr {
	top:0;
	left:0;
	width:30px;
	height:60px;
	line-height:60px;
	text-align:center;
	display:block;
	font-family:thinfont;
	font-size:24px;
	color:#787979;
}
.eoa-data-list .add .arr {
	color:#89CDA7;
	font-weight:bold;
	font-size:30px;
}
.eoa-data-list li .ico {
	top:0;
	right:10px;
	width:30px;
	height:60px;
	line-height:60px;
	text-align:center;
	display:block;
	font-family:simsun;
	font-size:24px;
	color:#787979;
}
.eoa-data-list li .detail {
	display:none;
	position:relative;
	background:#4a4949;
	color:#fff;
	padding:15px 15px;
	font-size:12px;
	line-height:180%;
}
.eoa-data-list li .detail .arrow {
	position:absolute;
	right:25px;
	top:-6px;
	display:block;
	width:0px;
	height:0px;
	border-top:0px solid #fff;
	border-bottom:6px solid #4a4949;
	border-left:6px solid #fff;
	border-right:6px solid #fff;
}
.eoa-data-list li .detail .hd,
.eoa-data-list li .detail .ft {
	margin:10px 0;
}
.eoa-data-list li .detail .attach {
	
}
.eoa-data-list li .detail .attach li {
	display:-webkit-box;
	line-height:20px;
}
.eoa-data-list li .detail .attach li  > * {
	-webkit-box-flex:1;
	-moz-box-flex:1;
	display:block;
}
.eoa-data-list li .detail .attach li  > .download {
	width:50px;
	min-width:50px;
	max-width:50px;
	text-align:right;
	color:#89CDA7;
}
.eoa-data-list li  .delete {
	color:#cc0000;
	font-family:thinfont;
}
.eoa-data-list ol {
	margin-bottom:10px;
}
.eoa-data-list ol > li {
	line-height:180%;
}
.eoa-data-list .nulldata {
	display:block;
	line-height:35px;
	text-align:center;
	background:#f4f4f4;
}
#eoa-detail {
	line-height:180%;
}
#eoa-detail img {
	width:100%;
	margin:5px auto;
	display: -webkit-box;
}

.function {
	padding-bottom:20px;
}
.function ul {
	margin:10px 0;
	box-shadow: inset 0 1px 0 rgba(255,255,255,.2), 0 1px 0px rgba(213,213,213,1);
}
.function ul li {
	position:relative;
	background:#fff;
	box-shadow:0 1px 1px rgba(213,213,213,0.5) inset;
}

.function ul li  .link {
	display:block;
	padding-left:55px;
	font-size:16px;
	height:50px;
	line-height:50px;
	color:#2f2f2f;
	background-size:30px 30px;
	background-repeat:no-repeat;
	background-position:15px center;
}
.function ul li .arr,
.function ul li .add {
	position:absolute;
	display:block;
	width:60px;
	height:50px;
	line-height:50px;
	top:0px;
	right:0px;
	text-align:center;
	font-size:30px;
	color:#787979;
}
.function ul li .add {
	font-family:thinfont;
}
.function ul li .arr {
	font-family:simsun;
	font-size:22px;
	font-weight:bold;
	background:url(../images/attend_icon_arr..png) no-repeat center;
}
.function .approve-list li {
	display:none;
}
.function .attend-record .link {
	background-image:url(../images/attend_icon_function1.png);
}
.function .apply-leave .link{
