}
.function .document-income-todo .link {
	background-image:url(../images/document_icon_income_todo.png);
}
.function .document-income-done .link {
	background-image:url(../images/document_icon_income_done.png);
}

#pageindex {
	background:#fff;
	position: relative;
	max-width:640px;
	min-height:100%;
	margin:0 auto;
	padding:15px 0;
	padding-bottom:5px;
	background-size:110% 100%;
}

#daily .form {
	display:none;
	margin-bottom:25px;
}
#daily .form textarea {
	width:97%;
	padding:5px;
	border:none;
	background:#f4f4f4;
	margin-bottom:15px;
	min-height:80px;
	line-height:150%;
}
#daily .form .btn {
	background-color:rgb(201,169,108);
	display:block;
	height:45px;
	line-height:45px;
	text-align:center;
	text-decoration:none;
	font-size:20px;
	color:#fff;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
#daily .attachment {
	display:none;
	margin:10px 0;
	font-size:12px;
}
#daily .attachment ol > li{
	display: -webkit-box;
	font-size: 14px;
	line-height: 130%;
	color: #4F4F4F;
	margin: 5px 0;
}
#daily .attachment ol li  > span {
	display:block;
	-webkit-box-flex:1;
	-moz-box-flex:1;
}
#daily .attachment ol li  > span.label {
	width:56px;
	min-width:56px;
	max-width:56px;
	text-align:left;
}
#daily .attachment ol li  > span.txt {
	word-break:break-all;
}
#daily .attachment ol li  > span.doc {
	background:url(../images/email_icon_attach.png) no-repeat left;
	padding-left:20px;
}
#daily .attachment ol li  > .download {
	display:none;
	width:40px;
	min-width:40px;
	max-width:40px;
	text-align:right;
	vertical-align:middle;
	color:#23B398;
	
}
#daily .list {
	margin-top:0px;
}
#daily .list > li {
	display:-webkit-box;
	padding-top:5px;
}
#daily .list > li:first-child {
	padding-top:0;
}

#daily .list > li.hd .content {
	padding-bottom:15px;
}
#daily .list > li .side {
	-webkit-box-flex:1;
	-moz-box-flex:1;
	width:80px;
	max-width:80px;
	min-width:80px;
	font-size:28px;
}
#daily .list > li .side .m {
	font-size:12px;
	font-weight:normal;
}
#daily .list > li .content-wrap {
	-webkit-box-flex:5;
	-moz-box-flex:5;
}
#daily .list > li .content {
	padding-bottom:0px;
	border-bottom:0px solid #92a6ae;
	/*box-shadow: inset 0 -1px 0px #d8dfe2,
                0 0px 0px 0px rgba(0, 0, 0, .1);*/
				
}

#daily .list > li.hd .btn {
	display:inline-block;
	width:90px;
	height:88px;
	overflow:hidden;
	background:#f4f4f4;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	margin-right:20px;
	text-align:center;
}
#daily .list > li.hd .btn:hover {
	background:#e4e4e4;
}
#daily .list > li.hd .btn .txt {
	-webkit-border-radius: 5px 5px 0 0;
	-moz-border-radius: 5px 5px 0 0;
	border-radius: 5px 5px 0 0;
	display:block;
	line-height:40px;
	background:#fe5768;
	color:#fff;
}
#daily .list > li.hd .btn .ico {
	display:block;
	height:45px;
	background:url(../images/daily_icon_edit.png) no-repeat center;
	background-size:28px 28px;
	-moz-background-size:28px 28px;
}
#daily .list > li.hd .btn-share .txt {
	background:#59b386;
}
#daily .list > li.hd .btn-share .ico {
	background:url(../images/daily_icon_share.png) no-repeat center;
	background-size:42px 28px;
	-moz-background-size:42px 28px;
}
#daily .list > li .t {
	position:relative;
	line-height:30px;
	font-size:12px;
}
#daily .list > li .t .n {
	font-size:16px;
	color:#5282aa;
}
#daily .list > li .t .del,
#daily .list > li .t .edit {
	position:absolute;
	top:0;
	right:0px;
	font-size:12px;
	color:#59b386;
}
#daily .list > li .t .date {
	position:absolute;
	top:0;
	right:0px;
	font-size:14px;
	color:#59b386;
}
#daily .list > li .b {
	line-height:150%;
	font-size:14px;
	color:#3A3A3A;
	overflow:hidden;
}
#daily .list > li .b:hover {
	background:#f6f6f6;
}
#daily .list > li .info {
	display:-webkit-box;
	line-height:35px;
}
#daily .list > li .info > li {
	-webkit-box-flex:1;
	-moz-box-flex:1;
}
#daily .list > li .info .date {
	font-size:14px;
	color:#59b386;
}
#daily .list > li .info .link {
	text-align:right;
}
#daily .list > li .info .comment {
	display:inline-block;
	background:url(../images/daily_icon_comment.png) no-repeat left;
	padding-left:20px;
	font-size:12px;
	vertical-align:middle;
}
#daily .list .nulldata {
	display:block;
	line-height:35px;
	text-align:center;
	background:#f4f4f4;
}


#daily  .comment-list {}
#daily  .comment-list .hd h2 {
	background:#E0DFDF;
	color:#747474;
	padding:4px 10px;
	font-size:14px;
}
#daily  .comment-list .bd {
	padding-bottom:10px;
	line-height:150%;
	font-size:13px;
}
#daily  .comment-list li {
	margin-top:10px;
}
#daily  .comment-list .reply {
	font-size:14px;
	font-weight:normal;
}
#daily  .comment-list .rer,
#daily  .comment-list .cer {
	font-size:14px;
	color:#5282aa;
	font-weight:bold;
}
#daily  .comment-list .b {
	font-size:13px;
	
}
#daily  .comment-list li:hover {
	background:#f6f6f6;
}

/*********************************
*********考勤管理界面样式属于高分辨率*************
*********所有数值在mdpi程度上增加为1.5倍*************
*******************************/
#attend #pageindex {
	margin-top:0;
	padding-top:0;
}
#attend .clock-wrap {
	padding:30px 100px 45px 90px;
	background:#2B87D6 url(../images/box_line1.png) repeat-x left bottom;
}
#attend .clock {
	position:relative;
	padding:15px;
	background:#F3F3F3;
}
#attend .clock .more {
	position:absolute;
	top:45px;
	right:-54px;
	width:54px;
	height:61px;
	display:block;
	background:url(../images/attend_icon_more.png) no-repeat;
}
#attend .clock-panel {
	background-color:#dddddd;
	box-shadow:0 1px 1px rgba(0,0,0,0.08) inset, 0 1px 1px #fafafa;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	height:120px;
}
#attend .clock-week {
	display:-webkit-box;
	padding:5px 30px;
}
#attend .clock-week  span {
	display:block;
	font-size:15px;
	color:#C3C3C3;
	text-align:center;
	-webkit-box-flex:1;
	-moz-box-flex:1;
	padding-top:15px;
}
#attend .clock-week  span.selected {
	color:#201E25;
}
#attend .time {
	font-family:digits;
	font-size:70px;
	text-align:center;
}
#attend .attend-list {
	padding:30px 15px;
	padding-bottom:10px;
}
#attend .attend-list li {
	background:#E6E6E6;
	padding:15px;
	padding-bottom:30px;
	margin-bottom:30px;
}
#attend .attend-list .times {
	font-size:18px;
	line-height:45px;
}
#attend .attend-list .operate {
	position:relative;
	display:-webkit-box;
