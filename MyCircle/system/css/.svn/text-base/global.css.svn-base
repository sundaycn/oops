}
.function .apply-out .link {
	background-image:url(../images/attend_icon_function3.png);
}
.function .approve-attend .link {
	background-image:url(../images/attend_icon_function10.png);
}
.function .approve-leave .link {
	background-image:url(../images/attend_icon_function11.png);
}
.function .approve-away .link {
	background-image:url(../images/attend_icon_function12.png);
}
.function .approve-over .link {
	background-image:url(../images/attend_icon_function13.png);
}
.function .approve-out .link {
	background-image:url(../images/attend_icon_function14.png);
}



.function .email-add .link {
	background-image:url(../images/email_icon_function1.png);
}
.function .email-inbox .link {
	background-image:url(../images/email_icon_function2.png);
}
.function .email-outbox .link {
