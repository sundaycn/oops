}
.theme-default#page nav  ul  li {
	text-align:center;
}
.theme-default#page nav  ul  li .link-index {
	width:60%;
	height:60px;
	display:block;
	margin:0 auto;
	text-align:center;
	overflow:hidden;
	text-indent:-999999px;
	background:url(../images/icon_home.png) no-repeat center 15px;
	background-size:30px 30px;
}
.theme-default#page .pager{
	display: -webkit-box;
    display: -moz-box;
    -webkit-box-orient: horizontal;
    -moz-box-orient: horizontal;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
	margin:5px;
}
.theme-default#page .list {
