}
.theme-default#pageindex  nav li.module-news {
	background-color:#feb528;
}
.theme-default#pageindex  nav li.module-news .icon {
	background:url(../images/icon_news.png) no-repeat center;
	background-size:60px 20px;
	height:85px;
}
.theme-default#pageindex  nav li.module-product {
	background-color:#05d09b;
}
.theme-default#pageindex  nav li.module-product .icon {
	background:url(../images/icon_product.png) no-repeat center;
